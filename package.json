{
  "name": "arquitetura-limpa-hexagonal",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "migrate:make": "knex migrate:make --knexfile ./src/exemplo/adapters/db/knex/knexfile.ts --knexpath ./src/exemplo/adapters/db/knex",
    "migrate:down": "knex migrate:rollback --knexfile ./src/exemplo/adapters/db/knex/knexfile.ts",
    "migrate:up": "knex migrate:latest --knexfile ./src/exemplo/adapters/db/knex/knexfile.ts",
    "migrate:restart": "npm run migrate:down && npm run migrate:up"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
